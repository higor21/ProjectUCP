module Processador_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg rst;
reg start;
// wires                                               
wire [6:0] Q1;
wire [6:0] Q2;

wire sampler;                             

// assign statements (if any)                          
Processador i1 (
// port map - connection between master ports and signals/registers   
	.clk(clk),
	.Q1(Q1),
	.Q2(Q2),
	.rst(rst),
	.start(start)
);

// clk
always
begin
	clk = 1'b0;
	clk = #5000 1'b1;
	#5000;
end 

// start
initial
begin
	start = 1'b0;
	start = #20000 1'b1;
	start = #60000 1'b0;
end 

// rst
initial
begin
	rst = 1'b0;
end 

Processador_vlg_sample_tst tb_sample (
	.clk(clk),
	.rst(rst),
	.start(start),
	.sampler_tx(sampler)
);

Processador_vlg_check_tst tb_out(
	.Q1(Q1),
	.Q2(Q2),
	.sampler_rx(sampler)
);
endmodule

